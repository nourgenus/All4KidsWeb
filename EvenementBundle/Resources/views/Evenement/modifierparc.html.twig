{% extends '::base_admin.html.twig' %}
{% block fos_user_content %}

    <section class="content">
        <div class="row">
            <div class="col-md-4">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header with-border">


                        <form role="form" method="post">
                            <div class="box-body">
                                <div class="form-group">
                                    <label for="exampleInputEmail1">nom:</label>

                                    <input type="text" name="nom" class="form-control" id="" value="{{ parcs.nom }}" placeholder="{{ parcs.nom }}">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">desciption:</label>

                                    <input type="text" name="description" class="form-control" id="" value="{{ parcs.decription }}" placeholder="{{ parcs.decription }}">
                                </div>

                                <div class="form-group">
                                    <label for="exampleInputEmail1">adresse:</label>

                                    <input type="text" name="adresse"  class="form-control" id="" value="{{ parcs.adresse }}" placeholder="{{ parcs.adresse }}">
                                </div>

                            </div>



                            <!-- /.box-body -->
                            <div class="box-footer">
                                <button type="submit" class="btn btn-primary">Modifier</button>
                            </div>



                        </form>
                    </div>
                </div>


            </div>
        </div>
    </section>
{% endblock %}
